<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout"
	xmlns:u="sap.ui.unified">
	<Dialog title="Enter details" id="fileUploader2">
		<f:SimpleForm>
			<l:VerticalLayout class="sapUiContentPadding" width="100%">
				<l:content>
					<VBox>
						<Label text="EmpId:"/>
						<Input id="empAddFId" required="true" width="100%" editable="false"/>
						<Label text="FName:"/>
						<Input id="fNameFId" required="true" width="100%"/>
						<Label text="LName:"/>
						<Input id="lNameFId" required="true" width="100%"/>
						<Label text="Department:"/>
						<Input id="empAdddepFId" required="true" width="100%"/>
						<Label text="Employee Pos:"/>
						<Input id="empAddposFId" required="true" width="100%"/>
						<Label text="Email:"/>
						<Input id="empAddEmailId" required="true" width="100%"/>
						<Label text="DOB:"/>
						<DatePicker id="empAddDOBId" placeholder="Enter Date ..." navigate="handleChangeDOB" class="sapUiSmallMarginBottom"/>
						<Label text="Starte date:"/>
						<DatePicker id="empAddStareId" placeholder="Enter Date ..." navigate="handleChangeStartDate" change="onchangeStartDate"
							class="sapUiSmallMarginBottom"/>
						<Label text="Starte date:"/>
						<DatePicker id="empAddEndId" placeholder="Enter Date ..." editable="false" class="sapUiSmallMarginBottom"/>
						<Label text="Password:"/>
						<Input id="empAddPassId" required="true" width="100%" editable="false"/>
						<Toolbar>
							<Button text="Add" press="onAddEmpClick" id="btnAddEmp"></Button>
							<Button type="Reject" text="Cancel" press="onCloseFragmentAddEmp"></Button>
						</Toolbar>
					</VBox>
				</l:content>
			</l:VerticalLayout>
		</f:SimpleForm>
	</Dialog>
</core:FragmentDefinition>