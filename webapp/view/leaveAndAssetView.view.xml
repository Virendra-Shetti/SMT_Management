<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns:tnt="sap.tnt" xmlns="sap.m"
	controllerName="MT.SMT_Managment.controller.leaveAndAssetView" xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page title="Title" showHeader="false">
				<tnt:ToolHeader>
					<tnt:ToolHeaderUtilitySeparator/>
					<ToolbarSpacer>
						<layoutData>
							<OverflowToolbarLayoutData priority="NeverOverflow" minWidth="20px"/>
						</layoutData>
					</ToolbarSpacer>
					<Button icon="sap-icon://log" press="onPressLogout">
						<layoutData>
							<OverflowToolbarLayoutData priority="NeverOverflow"/>
						</layoutData>
					</Button>
				</tnt:ToolHeader>
				<content>
					<IconTabBar id="idIconTabBarNoIcons" class="sapUiResponsiveContentPadding leftPadding" expanded="true">
						<items>
							<IconTabFilter class="leftPadding" text="Leave"></IconTabFilter>
							<IconTabFilter class="leftPadding" text="Asset"></IconTabFilter>
						</items>
					</IconTabBar>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>